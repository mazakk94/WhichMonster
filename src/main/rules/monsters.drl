package rules

import com.java_layer.*;

rule "When you were young"
	salience 1000
	when
		Fact(name("like_alternative_rock"), value(true))
		not Fact(name("song's_length"), value("long"))
		Fact(name("lame_song"), value(true))
		not Fact(name("without_solo"), value(false))
		not Fact(name("below500notes"), value(true))
		(or
			Fact(name("year"), value("sorc"))
			Fact(name("year"), value("druid"))
		) 
		not Fact(name("when_you_were_young"));
	then
		Communication.askQuestion("when_you_were_young");
end

